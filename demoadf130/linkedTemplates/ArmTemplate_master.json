{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "demoadf130"
		},
		"LS_Blob_demosa127_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_Blob_demosa127'"
		},
		"LS_Blob_dempsa127gen2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_Blob_dempsa127gen2'"
		},
		"LS_SQL_SourceDB_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_SQL_SourceDB'"
		},
		"LS_SQL_TargetDB_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_SQL_TargetDB'"
		},
		"LS_CosmosDB_OrderDB_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_CosmosDB_OrderDB'"
		},
		"LS_ADB_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'LS_ADB'"
		},
		"LS_DataBricks_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'LS_DataBricks'"
		},
		"LS_Blob_dempsa127gen2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://demosa127gen2.dfs.core.windows.net"
		},
		"LS_CosmosDB_OrderDB_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "orderdb"
		},
		"trigger2_properties_PL_CopyFileIfNotexists_parameters_FileName": {
			"type": "string",
			"defaultValue": "@triggerBody().fileName"
		},
		"trigger2_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/b55af1fd-af26-4506-a55a-3ce47e283afc/resourceGroups/demo_adf/providers/Microsoft.Storage/storageAccounts/demosa127"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "demoadf130_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_Blob_demosa127_connectionString": {
						"value": "[parameters('LS_Blob_demosa127_connectionString')]"
					},
					"LS_Blob_dempsa127gen2_accountKey": {
						"value": "[parameters('LS_Blob_dempsa127gen2_accountKey')]"
					},
					"LS_SQL_SourceDB_connectionString": {
						"value": "[parameters('LS_SQL_SourceDB_connectionString')]"
					},
					"LS_SQL_TargetDB_connectionString": {
						"value": "[parameters('LS_SQL_TargetDB_connectionString')]"
					},
					"LS_CosmosDB_OrderDB_connectionString": {
						"value": "[parameters('LS_CosmosDB_OrderDB_connectionString')]"
					},
					"LS_ADB_accessToken": {
						"value": "[parameters('LS_ADB_accessToken')]"
					},
					"LS_DataBricks_accessToken": {
						"value": "[parameters('LS_DataBricks_accessToken')]"
					},
					"LS_Blob_dempsa127gen2_properties_typeProperties_url": {
						"value": "[parameters('LS_Blob_dempsa127gen2_properties_typeProperties_url')]"
					},
					"LS_CosmosDB_OrderDB_properties_typeProperties_database": {
						"value": "[parameters('LS_CosmosDB_OrderDB_properties_typeProperties_database')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "demoadf130_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"trigger2_properties_PL_CopyFileIfNotexists_parameters_FileName": {
						"value": "[parameters('trigger2_properties_PL_CopyFileIfNotexists_parameters_FileName')]"
					},
					"trigger2_properties_typeProperties_scope": {
						"value": "[parameters('trigger2_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'demoadf130_ArmTemplate_0')]"
			]
		}
	]
}